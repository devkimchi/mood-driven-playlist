{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "name": {
      "value": "${AZURE_ENV_NAME}"
    },
    "location": {
      "value": "${AZURE_LOCATION}"
    },
    "authProviderName": {
      "value": "${CREDENTIAL_PROVIDER_NAME}"
    },
    "authProviderDisplayName": {
      "value": "${CREDENTIAL_PROVIDER_DISPLAY_NAME}"
    },
    "spotifyClientId": {
      "value": "${SPOTIFY_CLIENT_ID}"
    },
    "spotifyClientSecret": {
      "value": "${SPOTIFY_CLIENT_SECRET}"
    },
    "spotifyScopes": {
      "value": "${SPOTIFY_SCOPES}"
    },
    "spotifyAuthUrl": {
      "value": "${SPOTIFY_AUTH_URL}"
    },
    "spotifyTokenUrl": {
      "value": "${SPOTIFY_TOKEN_URL}"
    }
  }
}